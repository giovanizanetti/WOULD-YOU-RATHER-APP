 question2 = {\n    votes: votesOption2,\n    text: optionTwo.text,\n    isWinner: !isOption1Winner,\n    isEven: votesOption2 === votesOption1,\n    id: question_id,\n  }\n\n  const totalVotes = votesOption1 + votesOption2\n  const { name, avatarURL } = useSelector((state) => state.users[author]) || {}\n  const headerText = `Asked by ${name}`\n\n  return (\n    <Card name={name} avatar={avatarURL} headerText={headerText} isLargeHeader={true}>\n      {!isUserVote(optionOne) && !isUserVote(optionTwo) ? (\n        <ChooseOption options={{ question1, question2 }} />\n      ) : (\n        <div className='question-results-container'>\n          <h3>Results:</h3>\n          <QuestionResult question={question1} totalVotes={totalVotes} isUserVote={isUserVote(optionOne)} />\n          <QuestionResult question={question2} totalVotes={totalVotes} isUserVote={isUserVote(optionTwo)} />\n        </div>\n      )}\n    </Card>\n  )\n}\n\nexport default QuestionDetails\n","/Users/giovanizanetti/Projects/would-you-rather-app/src/components/utils/QuestionResult.js",[],"/Users/giovanizanetti/Projects/would-you-rather-app/src/components/utils/ProgressBar.js",[],"/Users/giovanizanetti/Projects/would-you-rather-app/src/components/utils/Card.js",[],"/Users/giovanizanetti/Projects/would-you-rather-app/src/components/ChooseOption.js",["140"],"/Users/giovanizanetti/Projects/would-you-rather-app/src/components/AddQuestion.js",[],"/Users/giovanizanetti/Projects/would-you-rather-app/src/components/utils/404.js",["141"],"import { Link, useHistory } from 'react-router-dom'\n\nconst NotFound = () => {\n  const history = useHistory()\n  return (\n    <div>\n      <h1>404 - Not Found!</h1>\n      <button onClick={() => history.replace('/')} className='btn' to='/'>\n        Go Home\n      </button>\n    </div>\n  )\n}\n\nexport default NotFound\n","/Users/giovanizanetti/Projects/would-you-rather-app/src/components/LeaderBoard/LeaderBoardItem.js",[],"/Users/giovanizanetti/Projects/would-you-rather-app/